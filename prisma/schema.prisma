generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model easterEgg {
  id          Int      @id @default(autoincrement())
  titulo      String
  midia       String
  tipo        String
  descricao   String
  dificuldade String
  descoberto  Boolean  @default(true)
  autor       String
  verificado  Boolean  @default(true)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}